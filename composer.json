{
    "name": "dandyphuong99/payment-gateway",
    "description": "Multi payment gateway for Laravel",
    "keywords": [
        "dandyphuong99",
        "laravel",
        "payment-gateway"
    ],
    "type": "library",
    "autoload": {
        "psr-4": {
            "Dandyphuong99\\PaymentGateway\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "require": {
        "php": "^8.1",
        "laravel/framework": "^12.0"
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/dandyphuong99/payment-gateway"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true
}